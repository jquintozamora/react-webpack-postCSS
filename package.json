    "css-loader": "~0.25.0",
    "extract-text-webpack-plugin": "~1.0.1",
    "opener": "~1.4.2",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.0.0",
    "postcss-loader": "^1.2.0",
    "react-hot-loader": "~3.0.0-beta.6",
    "style-loader": "~0.13.1",
    "webpack": "~1.13.3",
    "webpack-dev-server": "~1.16.2",
    "webpack-visualizer-plugin": "^0.1.5"
